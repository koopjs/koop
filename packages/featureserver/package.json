{
  "name": "featureserver",
  "version": "1.0.2",
  "description": "*An open source implementation of the GeoServices specification*",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "buble -i src -o dist && cp -r src/templates dist",
    "package": "npm run clean && npm run compile",
    "test": "standard && mocha test"
  },
  "author": "Daniel Fenton <dfenton@esri.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "esri-extent": "^1.1.1",
    "lodash": "^4.12.0",
    "moment": "^2.13.0",
    "terraformer-arcgis-parser": "^1.0.4",
    "winnow": "^1.0.1"
  },
  "devDependencies": {
    "async": "^2.1.4",
    "buble": "^0.15.1",
    "express": "^4.14.0",
    "mocha": "^2.4.5",
    "should": "^8.3.1",
    "standard": "^8.0.1",
    "supertest": "^2.0.1"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "after",
      "beforeEach",
      "afterEach"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FeatureServer/FeatureServer.git"
  },
  "keywords": [
    "featureserver",
    "geoservices",
    "geojson",
    "sql"
  ],
  "bugs": {
    "url": "https://github.com/FeatureServer/FeatureServer/issues"
  },
  "homepage": "https://github.com/FeatureServer/FeatureServer#readme"
}
