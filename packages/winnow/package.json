{
  "name": "@koopjs/winnow",
  "version": "4.2.1",
  "description": "Apply sql-like filters to GeoJSON",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tape '**/*.spec.js' --reporter=silent | tap-min",
    "test:unit": "tape 'src/**/*.spec.js' | tap-min",
    "benchmark": "node benchmark/index.js",
    "benchmark:ci": "node benchmark/index.js | tee benchmark-result.txt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koopjs/winnow.git"
  },
  "keywords": [
    "geojson",
    "filter",
    "stream",
    "sql",
    "geoservices",
    "query"
  ],
  "contributors": [
    {
      "name": "Rich Gwozdz",
      "email": "rgwozdz@esri.com"
    },
    {
      "name": "Daniel Fenton"
    }
  ],
  "license": "Apache-2.0",
  "files": [
    "src/**/*.js",
    "index.d.ts",
    "!**/*.spec.js"
  ],
  "bugs": {
    "url": "https://github.com/koopjs/koop/issues"
  },
  "homepage": "https://github.com/koopjs/koop#readme",
  "dependencies": {
    "@esri/proj-codes": "^3.2.0",
    "@koopjs/logger": "5.0.0",
    "@terraformer/arcgis": "^2.1.2",
    "@terraformer/spatial": "^2.1.2",
    "@turf/bbox-polygon": "^6.5.0",
    "@turf/centroid": "^6.5.0",
    "@types/geojson": "^7946.0.14",
    "alasql": "^4.3.0",
    "classybrew": "0.0.3",
    "joi": "^17.12.2",
    "js-sql-parser": "^1.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "murmurhash": "^2.0.1",
    "ngeohash": "^0.6.3",
    "proj4": "^2.10.0",
    "simple-statistics": "^7.8.3",
    "wkt-parser": "^1.3.3"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "fs-extra": "^11.2.0",
    "proxyquire": "^2.1.3",
    "sinon": "^17.0.1",
    "tap-min": "^3.0.0",
    "tape": "^5.7.5"
  },
  "optionalDependencies": {
    "farmhash": "^3.3.0"
  },
  "types": "index.d.ts"
}
