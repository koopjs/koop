{
  "name": "@koopjs/winnow",
  "version": "2.5.2",
  "description": "Apply sql-like filters to GeoJSON",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tape '**/*.spec.js' | tap-spec",
    "test:cov": "nyc --reporter=text-summary --reporter=html tape '**/*.spec.js' | tap-spec",
    "test:cov:unit": "nyc --reporter=text-summary --reporter=html tape 'lib/**/*.spec.js' | tap-spec",
    "benchmark": "node benchmark/index.js",
    "benchmark:ci": "node benchmark/index.js | tee benchmark-result.txt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koopjs/winnow.git"
  },
  "keywords": [
    "geojson",
    "filter",
    "stream",
    "sql",
    "geoservices",
    "query"
  ],
  "contributors": [
    {
      "name": "Rich Gwozdz",
      "email": "rgwozdz@esri.com"
    },
    {
      "name": "Daniel Fenton"
    }
  ],
  "license": "Apache-2.0",
  "files": [
    "src/**/*.js",
    "index.d.ts",
    "!**/*.spec.js"
  ],
  "bugs": {
    "url": "https://github.com/koopjs/koop/issues"
  },
  "homepage": "https://github.com/koopjs/koop#readme",
  "dependencies": {
    "@esri/proj-codes": "^2.6.0",
    "@terraformer/arcgis": "^2.1.1",
    "@terraformer/spatial": "^2.1.2",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/centroid": "^6.0.0",
    "@types/geojson": "^7946.0.7",
    "alasql": "^1.0.0",
    "classybrew": "0.0.3",
    "joi": "^17.6.3",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "ngeohash": "^0.6.3",
    "pgsql-ast-parser": "^10.0.2",
    "proj4": "^2.6.2",
    "simple-statistics": "^7.0.0",
    "string-hash": "^1.1.3",
    "wkt-parser": "^1.2.2"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "fs-extra": "^9.0.1",
    "proxyquire": "^2.1.3",
    "sinon": "^9.0.3",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.0"
  },
  "optionalDependencies": {
    "farmhash": "^3.1.0"
  },
  "types": "index.d.ts"
}
